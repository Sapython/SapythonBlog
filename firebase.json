{
  "hosting": {
    "public": "dist/static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(jpg|webp|jpeg|gif|png|svg|webp|js|css|eot|otf|ttf|ttc|woff|woff2|font.css)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=36000"
      }]
    }],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
